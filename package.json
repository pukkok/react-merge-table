{
  "name": "auto-merge-table",
  "version": "1.0.4",
  "description": "React component for auto-merging rowspan/colspan in HTML tables",
  "author": "pukkok <krystalgggg@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffect": false,
  "keywords": [
    "react",
    "table",
    "react-table",
    "rowspan",
    "colspan",
    "cell merging",
    "auto merge",
    "merge table",
    "spreadsheet",
    "component",
    "data table"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": ">=18.0.0 <20.0.0",
    "react-dom": ">=18.0.0 <20.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.4.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "tsup": "^7.2.0"
  }
}
